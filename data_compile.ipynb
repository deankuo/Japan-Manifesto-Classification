   "source": [
    "# Divided the file name into year, state, district, and candidate name\n",
    "for year in years:\n",
    "    for i in globals()[f\"df_{year}\"].index:\n",
    "        globals()[f\"df_{year}\"].loc[i, 'year'] = globals()[f\"df_{year}\"].loc[i, 'file_name'].split('_')[0] # type: ignore\n",
